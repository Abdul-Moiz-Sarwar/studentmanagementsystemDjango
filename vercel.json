{
    "version": 2,
    "builds": [
      {
        "src": "StudentManagementSystem/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      },
      {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "outputDirectory": "staticstaticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "StudentManagementSystem/wsgi.py"
      }
    ]
  }